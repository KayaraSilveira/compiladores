main{
    var int n;
    n := 20;
    if((n == 1)){
        print(1);
    };
    if((n > 1)){
        fibonacci(n);
    };
}

function int fibonacci(int n){
    var int a;
    var int b;
    var int fibo;
    a := 0;
    b := 1;
    while((n > 1)) do
    {
        fibo := (a + b);
        a := b;
        b := fibo;
        n := (n - 1);
    };
    print(fibo);
}
